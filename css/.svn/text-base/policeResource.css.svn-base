.ul{display:block;width:33%;position:absolute;}
i{color:#fff;font-size: 3em;}
ul>li{float:left;margin:20px 45px;width:958px;height:453px;overflow: hidden;}
ul>li .li_title{text-align: center;font-size: 3.7em;line-height:190px;}
ul>li p span{line-height:245px;position: relative;display: inline-block;text-align: center;width:30%;}
.center_ul>li p span{line-height:160px;}
ul>li p span .icon{width:70px;position:absolute;top:35%;}
ul .bg p span .icon{top:26%;}
ul>li p span i{padding-left: 25%;}
ul>li p span .li_phone{padding-left:18%;}
ul>li .mini span .icon{width:50px;top:13%;}
ul>li p span .radio{width: 55px;height: 80px;}
ul>li .mini span .radio{width:45px;height:60px;}
/*不同类型的标题色*/
ul>li .yellow{color: #feef15;}
ul>li .orange{color:#fca301;}
ul>li .green{color:#a5fe0e;font-size: 3.2em;}
ul>li .red{color:#ff4848;}
/*不同类型的背景*/
ul .leftbg1{background: url(../image/jwzy_bg4.png) no-repeat;}
ul .leftbg2{background:url(../image/jwzy_bg6.png) no-repeat;}
ul .leftbg3{background: url(../image/jwzy_bg5.png) no-repeat;}
ul .bg{background: url(../image/jwzy_bg1.png) no-repeat;width:1001px;height:294px;margin:20px 40px;}
ul .rightbg{background: url(../image/jwzy_bg3.png) no-repeat;}
/*地图div*/
.center_div .map_div{border:5px solid #5ef0ff;margin: 0px 1.5% 0 1.3%;height: 1433px;}